    {{{body}}}
    <div class="container-fluid" id="mapa">
        <div class="container text-center">
            <div class="row">
                <div class="col">
                    <h3>Informacion</h3>
                    <p id="informacion"> E. Madero 1599. C.P. 1638 - Vicente López - Provincia de Buenos Aires</p>
                    <p id="informacion">Tel: 4791-2132 / 4796-4334</p>
                    <p id="informacion">Horarios de actividad:
                        Lunes - Martes - Jueves: De 20hrs a 22hrs (Primera - Sub 23)
                        Lunes - Miercoles: De 19hrs a 20.30hrs (Juveniles)
                    </p>
                </div>
                <div class="col-7" id="ubicacion">
                    <h4>Ubicacion del Club</h4>
                    <iframe
                        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3287.310391326395!2d-58.48143512477609!3d-34.520363253099966!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x95bcb14e8fa0baa1%3A0x8813e88409d82d54!2sClub%20Banco%20Ciudad!5e0!3m2!1ses!2sar!4v1683992553366!5m2!1ses!2sar"
                        width="600" height="300" style="border:0;" allowfullscreen="" loading="lazy"
                        referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
                <div class="col">
                    <h3>Contactate</h3>
                    <form class="row g-3" action="/" method="post">
                        <div class="col-md-6">
                            <label for="inputName4" class="form-label">Nombre</label>
                            <input type="name" class="form-control" id="inputName4" name="nombre">
                        </div>
                        <div class="col-md-6">
                            <label for="inputPassword4" class="form-label">Apellido</label>
                            <input type="surname" class="form-control" id="inputPassword4" name="apellido">
                        </div>
                        <div class="col-12">
                            <label for="inputEmail4" class="form-label">Email</label>
                            <input type="email" class="form-control" id="inputEmail4" name="email">
                        </div>
                        <div class="col-12">
                            <label for="exampleFormControlTextarea1" class="form-label">Opinion de nuestra
                                pagina</label>
                            <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="opinion"></textarea>
                        </div>
                        <div class="col-12">
                            <button class="btn btn-outline-success" type="submit" id="buscador">Enviar</button>
                        </div>
                    </form>
                    <p>{{message}}</p>
                </div>
            </div>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"
